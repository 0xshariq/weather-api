{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri",
      "API_KEY": "@api_key",
      "FRONTEND_URL": "@frontend_url",
      "PORT": "3000"
    }
  }
  
  